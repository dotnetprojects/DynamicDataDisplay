<UserControl x:Class="DateTimeAxisTutorial.Page"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
    xmlns:d3="clr-namespace:Microsoft.Research.DynamicDataDisplay;assembly=DynamicDataDisplaySilverLight"
    Width="900" Height="800">
    <Grid x:Name="LayoutRoot" Background="White">
        <d3:ChartPlotter Name="plotter" Width="800" Height="600" VerticalAlignment="Top">

            <d3:Header FontFamily="Georgia" Content="DateTimeAxis Tutorial"/>
            <d3:VerticalAxisTitle FontFamily="Georgia">
                <TextBlock>Vertical axis</TextBlock></d3:VerticalAxisTitle>
            <d3:HorizontalAxisTitle FontFamily="Georgia">
                <TextBlock>DateTime axis</TextBlock></d3:HorizontalAxisTitle>
        </d3:ChartPlotter>
    </Grid>
</UserControl>
