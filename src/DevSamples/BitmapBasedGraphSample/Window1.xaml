<Window x:Class="BitmapBasedGraphSample.Window1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d3="http://research.microsoft.com/DynamicDataDisplay/1.0"
    xmlns:local="clr-namespace:BitmapBasedGraphSample"
    Title="Window1" Height="300" Width="300">
    <Grid>
        <d3:ChartPlotter Name="plotter" VerticalAlignment="Stretch" HorizontalAlignment="Stretch">
            <local:BitmapBasedSampleGraph/>
        </d3:ChartPlotter>
    </Grid>
</Window>
