<Page x:Class="NewMarkersSample.Pages.EndlessRandomValuesPage"
	  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	  xmlns:d3="http://research.microsoft.com/DynamicDataDisplay/1.0"
	  Title="EndlessRandomValuesPage" Loaded="Page_Loaded">
    <Grid>
		<d3:ChartPlotter Name="plotter">
			<d3:DevMarkerChart Name="chart" IndependentValuePath="X" DependentValuePath="Y" 
							   d3:NewLegend.Description="Random points"
							   d3:Viewport2D.UseDeferredPanning="True">
				<d3:EllipseMarker Width="10" Height="10"/>
			</d3:DevMarkerChart>
		</d3:ChartPlotter>
	</Grid>
</Page>
